package com.Strings;

import java.util.Scanner;

public class LoginFunctionality {
	public static void main(String[] args) {
		String name="Dora";
		String password="Dora@123";
		
		
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a name");
		String n=sc.nextLine();
		System.out.println("enter a password");
		String p=sc.nextLine();
		
		
		public void LogInDetils() {

		
		if(n.equals(name)&& p.equals(password)) {
			
			
			System.out.println("login successfully completed");
		System.out.println("You are most welcome");
		}
		else 
		
			System.out.println("invalidÂ input");

}
	}
	
}
