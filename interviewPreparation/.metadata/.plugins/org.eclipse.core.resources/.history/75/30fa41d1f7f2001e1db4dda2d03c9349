package com.arrays.usingstaticmethods;

public class ArraysSortingByUsingStaticMethod {
	
	
	public static void main(String[] args) {
		
		ArraysSortingByUsingStaticMethod reference=new ArraysSortingByUsingStaticMethod();
		reference.metod1(5);
		
		
		
	}
	
	
	
	public static int[] metod1(int array){
	
	int[] arr = { 1, 7, 1, 7, 2, 5, 3, 9, 2, 1, 1, 3, 7 };

	int temp;

	for (int i = 0; i < arr.length - 1; i++) {

		for (int j = i + 1; j < arr.length; j++) {

			if (arr[i] == arr[j]) {
				temp = arr[j];

				for (int k = j; k > i; k--) {
					arr[k] = arr[k - 1]; // arr[2] =
				}

				arr[i + 1] = temp;
				break;

			}

		}

	}

	for (int value1 : arr) {
		System.out.print(value1 + " ");
	}
	return arr;


}
}
