package com.numberpattern;

public class NumberPatterns {
	
	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++) {
			//System.out.println(i);
			for(int j=i;j>=0;j--) {
				//System.out.println(i);
				//System.out.print(i);
				//System.out.println(j);
				System.out.print(j);



			}
			
			System.out.println(i);

			System.out.println();
		}
		
	}

}
