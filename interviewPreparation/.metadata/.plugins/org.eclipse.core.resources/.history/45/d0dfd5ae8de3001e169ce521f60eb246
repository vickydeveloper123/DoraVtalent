package com.numberpattern;

public class NumberPatterns {
	
	public static void main(String[] args) {
		
		for(int i=5;i>=1;i--) {
			//System.out.println(i);
			for(int j=1;j>=i;j++) {
				//System.out.println(i);
				//System.out.print(i);
				//System.out.println(j);
				System.out.print(j);



			}
			
			//System.out.print(i);

			System.out.println();
		}
		
	}

}
